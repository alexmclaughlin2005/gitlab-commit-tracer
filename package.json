{
  "name": "gitlab-commit-tracer",
  "version": "0.1.0",
  "description": "AI-powered GitLab commit monitoring and context analysis tool",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:ui": "cd ui && npm run dev",
    "build": "tsc",
    "build:server": "tsc",
    "build:ui": "cd ui && npm run build",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "gitlab",
    "commit",
    "ai",
    "analysis",
    "monitoring"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "express": "^4.19.2",
    "openai": "^6.9.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
